def calculate_sum(L):
    s = 0
    i = 0
    while i < 4:
        s = s + L[i]
        i = i + 1
    return s

def main():
    a = [1, 2, 3, 4, 5]
    b = [6, 7, 8, 9, 10]
    
    print("list a:", a)
    print("list b:", b)

    sum_a = calculate_sum(a)
    sum_b = calculate_sum(b)
    
    print("sum of list a:", sum_a)
    print("sum of list b:", sum_b)
    
    if sum_a > sum_b:
        print("list a has a greater sum")
    else:
        print("list b has a greater sum")
    
    a[0] = 10
    a[0] = a[0] + 5
    a[1] = a[1] - 2
    a[2] = a[2] * 2
    a[3] = a[3] // 2
    a[4] = a[4] % 3
    
    print("modified list a:", a)

    a[0] = a[0] * 2
    
    print("squared first element of list a:", a[0])

main()
